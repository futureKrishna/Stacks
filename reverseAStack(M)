void reverseStack(stack<int> &input, stack<int> &extra) {
    //Write your code here
    if(input.size()<=1)
    {
        return ;
    }
    
    int top= input.top();
    input.pop();
     reverseStack(input,extra);
    while(input.size()!=0)
    {
        extra.push(input.top());
        input.pop();
    }
    
    input.push(top);
    
       while(extra.size()!=0)
    {
        input.push(extra.top());
        extra.pop();
    }
}
